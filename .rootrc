# Specify port range for http server:
WebGui.HttpPortMin:         10000
WebGui.HttpPortMax:         10101

# Alternatively, fix port (single instance!):
# WebGui.HttpPort:    9090

# Enable https:
# WebGui.UseHttps:    yes
# WebGui.ServerCert: letsencrypt-server.pem
# Note, civet wants full cert chain++, e.g.:
#   cd /etc/letsencrypt/live/some-host.ucsd.edu/
#   cat privkey.pem fullchain.pem > /home/viz/revetor/letsencrypt-server.pem
